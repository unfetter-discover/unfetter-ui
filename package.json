{
  "name": "unfetter-ui",
  "version": "0.3.3",
  "description": "UNFETTER UI",
  "keywords": [
    "unfetter"
  ],
  "author": "UNFETTER",
  "homepage": "https://github.com/unfetter-discover/unfetter-ui",
  "license": "MIT",
  "scripts": {
    "analyze": "webpack-bundle-analyzer stats.json dist",
    "analyze:dev": "npm run clean && webpack --config config/webpack.dev.js --profile --json > stats.json && npm run analyze",
    "analyze:prod": "npm run clean && webpack --config config/webpack.prod.js --profile --json > stats.json && npm run analyze",
    "ng": "ng",
    "clean": "rimraf compiled",
    "start": "ng version && ng serve",
    "start:dev": "ng version && ng serve --ssl --disable-host-check --host 0.0.0.0 --port 80",
    "serve:demo:local": "ng version && ng serve -v -w -dev -sm --open --ssl --disable-host-check --port 9443 --host 0.0.0.0  --public-host 0.0.0.0:9443 --proxy-config proxy.conf.json",
    "serve:uac:local": "ng version && ng serve -v -w -dev -e uac -sm --open --ssl --port 9443 --disable-host-check --host 0.0.0.0 --public-host 0.0.0.0:9443 --proxy-config proxy.conf.json",
    "build:prod": "ng version && ng build --prod --env=prod",
    "build:dev": "ng version && ng build",
    "test": "ng test",
    "lint": "ng lint",
    "doc": "typedoc --json typedoc.json src",
    "checkup": "concurrently -k \"npm run lint\"  \"npm run doc\" \"npm run build:prod\" \"npm run test\"",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "@angular/cdk": "2.0.0-beta.12",
    "@angular/material": "2.0.0-beta.12",
    "core-js": "^2.4.1",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.14",
    "@ngrx/effects": "^4.1.1",
    "@ngrx/store": "^4.1.1",
    "@ngrx/store-devtools": "^4.1.1",
    "angular2-jwt": "^0.2.3",
    "bootstrap": "~3.3.7",
    "d3": "~4.10.0",
    "hammerjs": "2.0.8",
    "jquery": "3.2.1",
    "moment": "2.18.1",
    "ng2-charts": "^1.6.0",
    "ngx-bootstrap": "^1.9.3",
    "perfect-scrollbar": "~1.3.0",
    "primeng": "^5.0.0"
  },
  "devDependencies": {
    "@angular/cli": "~1.6.1",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/language-service": "^5.0.0",
    "@types/d3": "~4.10.0",
    "@types/hammerjs": "^2.0.33",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/jquery": "2.0.42",
    "@types/node": "~6.0.60",
    "codelyzer": "~4.0.1",
    "concurrently": "^3.5.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "rimraf": "~2.6.0",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typedoc": "^0.9.0",
    "typescript": "~2.4.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unfetter-discover/unfetter-ui"
  },
  "bugs": {
    "url": "https://github.com/unfetter-discover/unfetter/issues"
  },
  "engines": {
    "node": ">= 6.10.0",
    "npm": ">= 5"
  }
}
