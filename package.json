{
  "name": "unfetter-ui",
  "version": "0.3.6",
  "description": "UNFETTER UI",
  "keywords": [
    "unfetter"
  ],
  "author": "UNFETTER",
  "homepage": "https://github.com/unfetter-discover/unfetter-ui",
  "license": "MIT",
  "scripts": {
    "analyze": "webpack-bundle-analyzer dist/stats.json dist",
    "analyze:dev": "npm run clean && npm run build:dev -- --stats-json && npm run analyze",
    "analyze:prod": "npm run clean && npm run build:prod -- --stats-json && npm run analyze",
    "ng": "ng",
    "clean": "rimraf compiled dist tsdocs coverage out-tsc",
    "start": "npm run serve:local:dev:uac",
    "start:dev": "ng version && ng serve --ssl --disable-host-check --host 0.0.0.0 --port 80",
    "serve:local:prod:demo": "ng version && ng serve -v -w --prod --sourcemap --hmr --open --ssl --port 9443 --proxy-config proxy.conf.json",
    "serve:local:dev:demo": "ng version && ng serve -v -w --dev --sourcemap --hmr --open --ssl --port 9443 --proxy-config proxy.conf.json",
    "serve:local:dev:uac": "ng version && ng serve -v -w --dev -e uac --sourcemap --hmr --open --ssl --port 9443 --proxy-config proxy.conf.json",
    "serve:docker:dev:uac": "ng version && ng serve -v -w --dev -e uac -sm --hmr --open --ssl --port 80 --disable-host-check --host 0.0.0.0 --public-host 0.0.0.0:80",
    "serve:docker:dev:demo": "ng version && ng serve -v -w --dev -sm --open --hmr --ssl --disable-host-check --port 80 --host 0.0.0.0  --public-host 0.0.0.0:80",
    "super:test": "concurrently -k -n \"serve,test,coverage\" -c \"blue,green,red\" \"npm run serve:local:dev:uac\" \"ng test --code-coverage\" \"npm run serve:coverage\"",
    "live:test": "concurrently -k -n \"test,coverage\" -c \"green,red\" \"ng test --code-coverage\" \"npm run serve:coverage\"",
    "serve:coverage": "live-server coverage/",
    "build:prod": "ng version && ng build --prod --env=prod",
    "build:prod:noclean": "npm run build:prod -- --no-delete-output-path --vendor-chunk=true",
    "build:demo:noclean": "ng version && ng build --prod --no-delete-output-path --vendor-chunk=true --env=dockerhub",
    "build:dev": "ng version && ng build",
    "watch:test": "ng test --watch --source=true",
    "test": "ng test --single-run --source=false",
    "headless:test": "ng test --single-run --browsers ChromeHeadlessCustom",
    "coverage": "ng test --code-coverage --single-run",
    "headless:coverage": "ng test --code-coverage --single-run --browsers ChromeHeadlessCustom",
    "lint": "npm run lint:style && ng lint",
    "lint:style": "stylelint \"src/**/*.scss\" --syntax scss",
    "doc": "typedoc --out tsdocs --ignoreCompilerErrors src/",
    "checkup": "npm run lint && npm run doc && npm run headless:test && npm run headless:coverage && npm run build:prod",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/cdk": "^5.0.0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/material": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "@ngrx/effects": "^4.1.1",
    "@ngrx/store": "^4.1.1",
    "@ngrx/store-devtools": "^4.1.1",
    "angular2-jwt": "^0.2.3",
    "bootstrap": "~3.3.7",
    "clone": "^2.1.1",
    "core-js": "^2.4.1",
    "css-element-queries": "^1.0.2",
    "d3": "~4.12.0",
    "d3-heatmap": "^1.2.1",
    "hammerjs": "~2.0.8",
    "jquery": "~3.2.1",
    "moment": "~2.20.0",
    "ng2-charts": "^1.6.0",
    "ngx-clipboard": "^9.1.3",
    "perfect-scrollbar": "~1.3.0",
    "primeng": "^5.0.0",
    "rxjs": "^5.5.2",
    "stix": "https://github.com/unfetter-discover/stix/tarball/develop",
    "ts-keycode-enum": "^1.0.4",
    "uuid": "^3.2.1",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular-devkit/build-optimizer": "0.2.0",
    "@angular-devkit/core": "0.2.0",
    "@angular/cli": "1.6.7",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/language-service": "^5.0.0",
    "@angularclass/hmr": "^2.1.3",
    "@types/d3": "~4.12.0",
    "@types/d3-dsv": "^1.0.31",
    "@types/hammerjs": "^2.0.35",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/jquery": "~3.2.17",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "concurrently": "^3.5.1",
    "google-charts": "^1.0.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "live-server": "^1.2.0",
    "ngrx-store-hmr": "0.0.3",
    "protractor": "~5.1.2",
    "rimraf": "~2.6.0",
    "stylelint": "^9.2.0",
    "stylelint-scss": "^3.1.0",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typedoc": "^0.9.0",
    "typescript": "~2.6.2",
    "webpack-bundle-analyzer": "^2.9.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unfetter-discover/unfetter-ui"
  },
  "bugs": {
    "url": "https://github.com/unfetter-discover/unfetter/issues"
  },
  "engines": {
    "node": ">= 6.10.0",
    "npm": ">= 5"
  }
}
